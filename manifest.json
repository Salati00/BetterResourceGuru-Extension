{
  "name": "ResourceGuru Quote remover",
  "version": "1.5",
  "manifest_version": 2,
  "description": "Remove motivational quotes from resource guru",
  "homepage_url": "https://www.linkedin.com/in/giorgiosalati/",
  "content_scripts": [
    {
      "matches": ["*://*.resourceguruapp.com/*"],
      "css": ["inject.css"]
    }
  ],
  "browser_action": {
    "default_title": "BegoneGuru"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}